{
    "name": "magnafacta/zalt-loader",
    "description": "Zend Alternative Loader",
    "type": "library",
    "require": {
        "php": ">=8.1.0",
        "psr/container": "^1.0 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "9.*"
    },
    "authors": [
        {
            "name": "Matijs de Jong",
            "email": "mjong@magnafacta.nl"
        },
        {
            "name": "Menno Dekker",
            "email": "menno.dekker@erasmusmc.nl"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Zalt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zalt\\": "test\\"
        }
    },
    "repositories": [
    ]
}
